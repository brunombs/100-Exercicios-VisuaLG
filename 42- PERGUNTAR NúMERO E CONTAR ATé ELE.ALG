algoritmo "Perguntar número e contar até ele"
var
   numero, contador: inteiro
inicio
      contador <- 1
      escreval ("Escolha um número positivo e contaremos do 0 até ele :)")
      leia (numero)
      enquanto (contador <= numero) faça
      escreval (contador)
      contador <- contador + 1
      fimenquanto
      Escreval ("Contagem realizada com sucesso. ˆˆ")
fimalgoritmo