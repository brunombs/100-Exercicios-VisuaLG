algoritmo "Ler dois números inteiros e exibir o somatório"
var
   n1, n2, t: inteiro
inicio
      Escreval ("Qual é o primeiro número?")
      Leia (n1)
      Escreval ("Qual é o segundo número?")
      Leia (n2)
      t <- n1 + n2
      Escreval ("A soma entre ", n1, " e" ,n2 , " é igual a", t)
fimalgoritmo
